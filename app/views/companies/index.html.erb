<h1>Companies</h1>
<table>
  <th class="col-large">
    <h3><tr align="left">
      <td>Name</td>
      <td>City</td>
      <td>State</td>
      <td>Url</td>
      <td>About</td>
    </tr></h3>
  </th>
<% @companies.each do |company| %>
  <tr>
    <td><%= link_to company.name, company_path(company) %></td>
    <td><%= company.city %></td>
    <td><%= company.state %></td>
    <td><%= link_to company.name, company.url %></td>
    <td><%= company.about %></td>
    <td>
  </tr>
<% end %>
</table>
